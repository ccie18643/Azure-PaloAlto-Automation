    "contentVersion": "1.0.0.0",
    "parameters": {
        "network_id": {
            "type": "String",
            "defaultValue": null
        },
        "site_id": {
            "type": "string",
            "defaultValue": null
        },
        "er_circuit_id": {
            "type": "string",
            "defaultValue": null
        },
        "er_auth_key": {
            "type": "string",
